                    { extend: 'excel', text: '<i  aria-hidden="true">EXCEL</i>' },
                    { extend: 'print', text: '<i  aria-hidden="true">IMPRIMIR</i>' },
                    { extend: 'copy', text: '<i  aria-hidden="true">COPIAR</i>' },
                    'pageLength'
                ],
            });
            table.buttons().container()
                .appendTo('#datatable_wrapper .col-md-6:eq(0)');
        });
    </script>

