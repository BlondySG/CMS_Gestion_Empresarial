                                <dd class="col-sm-10">
                                    @Html.DisplayFor(model => model.IdProveedor)
                                </dd>
                                <dt class="col-sm-2">
                                    @Html.DisplayNameFor(model => model.NombreProveedor)
                                </dt>
                                <dd class="col-sm-10">
                                    @Html.DisplayFor(model => model.NombreProveedor)
