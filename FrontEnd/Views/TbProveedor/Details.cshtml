                                </dd>
                                <dt class="col-sm-2">
                                    @Html.DisplayNameFor(model => model.Activo)
                                </dt>
                                <dd class="col-sm-10">
                                    @Html.DisplayFor(model => model.Activo)
                                </dd>
                            </dl>
                        </div>
                    </div>
                    <br />
                    <div>
                        @Html.ActionLink("Editar", "Edit", new { id = Model.IdProveedor }, new { @class="btn btn-warning"}) |
                        @Html.ActionLink("Regresar", "Index","TbProveedor", null, new { @class="btn btn-primary"})
